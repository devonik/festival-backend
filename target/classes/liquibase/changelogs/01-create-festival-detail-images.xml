<?xml version="1.0" encoding="UTF-8"?> 
<databaseChangeLog 
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd"> 
                      
     <changeSet id="5" author="nik">
        <createTable tableName="festival_detail_images">
            <column autoIncrement="true" name="festival_detail_images_id" type="BIGINT">
                <constraints primaryKey="true"/>
            </column>
            <column name="festival_detail_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="title" type="VARCHAR(45)"/>
            <column name="url" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="description" type="LONGTEXT"/>
        </createTable>
    </changeSet>
    
</databaseChangeLog> 